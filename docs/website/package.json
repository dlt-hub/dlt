{
  "name": "dlt-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run gen-api-ref && npm run preprocess-docs &&concurrently --kill-others \"PYTHONPATH=. uv run python -u -m tools.preprocess_docs --watch\" \"docusaurus start\"",
    "build": "npm run update-versions && npm run preprocess-docs && npm run gen-api-ref && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "clear-versions": "node tools/clear_versions.js",
    "update-versions": "node tools/update_versions.js",
    "gen-api-ref": "PYTHONPATH=. uv run pydoc-markdown && uv run python clean_pydoc_sidebar.py",
    "gen-api-ref:cloudflare": "PYTHONPATH=. pydoc-markdown && python clean_pydoc_sidebar.py",
    "preprocess-docs": "PYTHONPATH=. uv run python -u -m tools.preprocess_docs",
    "preprocess-docs:cloudflare": "PYTHONPATH=. python -u -m tools.preprocess_docs",
    "render-cli-docs": "PYTHONPATH=. uv run dlt render-docs > docs/reference/command-line-interface-generated.md",
    "build:cloudflare": "npm run update-versions  && npm run preprocess-docs:cloudflare && npm run gen-api-ref:cloudflare &&  docusaurus build --out-dir build/docs",
    "build:netlify": "npm run update-versions && npm run preprocess-docs:cloudflare && npm run gen-api-ref:cloudflare &&  docusaurus build --out-dir build/docs",
    "serve:cloudflare": "wrangler dev --port 3001 --local",
    "preview:cloudflare": "npm run build:cloudflare && npm run serve:cloudflare",
    "deploy:cloudflare": "npm run build:cloudflare && wrangler deploy"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-common": "^3.7.0",
    "@mdx-js/react": "^3.1.0",
    "@microlabs/otel-cf-workers": "1.0.0-rc.49",
    "clsx": "^1.2.1",
    "dedent": "^1.5.1",
    "dotenv": "^16.3.1",
    "node-watch": "^0.7.4",
    "prism-react-renderer": "^1.3.5",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-twitter-embed": "^4.0.4",
    "semver": "^7.6.3",
    "string-dedent": "^3.0.1",
    "sync-fetch": "^0.5.2",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "concurrently": "^8.2.2",
    "wrangler": "^4.38.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.10"
  }
}
