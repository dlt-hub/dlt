{
    "name": "docs",
    "compatibility_date": "2025-09-19",
    "main": "worker.js",
    "preview_urls": true,
  	"compatibility_flags": [
		"nodejs_compat_v2"
	],
    "assets": {
        "directory": "./build",
        "run_worker_first": ["/*"],
        "binding": "ASSETS" // <-- creates env.ASSETS variable
    },
    "observability": {
      "enabled": true,
      "head_sampling_rate": 1
    },
  	"vars": {
      "AXIOM_DATASET": "vibes",
      "AXIOM_URL": "https://api.axiom.co/v1/traces",
      "ENV": "production"
    },
    "routes": [
        {
          "pattern": "stage.dlthub.com/docs*",
          "zone_name": "dlthub.com"
        },
        {
          "pattern": "dlthub.com/docs*",
          "zone_name": "dlthub.com"
        }
      ]
  }
  